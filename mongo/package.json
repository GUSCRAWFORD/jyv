{
  "name": "@jyv/mongo",
  "version": "0.0.0",
  "description": "Out-of-the-box Generic Repository via MongoClient for NodeJS",
  "main": "./dist/bin/main.js",
  "types": "./dist/main.d.ts",
  "repository": "https://github.com/GUSCRAWFORD/jyve",
  "author": "Gus",
  "license": "MIT",
  "private": false,
  "scripts": {
    "postinstall": "echo \"âœ…  Replacing @jyv/core\" && rm -rfv node_modules/@jyv && cd ../core && yarn build && cd ../mongo && fpm add ../core",
    "clean": "echo \"ðŸ—‘  Cleaning build...\" && rm -rfv dist",
    "build": "echo \"ðŸ›   Building...\" && tsc -diagnostics && fpm transform .",
    "rebuild": "yarn clean && yarn build"
  },
  "peerDependencies":{
    "@jyv/core":"0.0.0",
    "@types/node": "^8.10.4",
    "mongodb": "^3.1.4"
  },
  "devDependencies": {
    "@guscrawford.com/fpm": "^0.0.10",
    "@types/node": "^8.10.4",
    "mongodb": "^3.1.4"
  },
  "@fpm:transform": {
    "scripts": "@fpm:remove",
    "devDependencies":"@fpm:remove",
    "main": {
      "@fpm:replace": {
        "dist/": ""
      }
    },
    "types": {
      "@fpm:replace": {
        "dist/": ""
      }
    }
  }
}
